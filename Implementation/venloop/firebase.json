{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/teamleader-task-submition/view",
        "destination": "/teamleader-task-submition/view.html"
      },
      {
        "source": "/teams/view",
        "destination": "/teams/view.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "admin-edit-task-template/view",
        "destination": "admin-edit-task-template/view/index.html"
      }

    ]
  }
}
